{	
	"method" : "get",
	"variables" : [
		{
			"name" : "id",
			"type" : "queryParam",
			"description" : "The ID of the post to fetch"
		}
	],
	"request" : {
		"uri" : "https://jsonplaceholder.typicode.com/posts/$(id)",
		"method" : "get",
		"schema" : {
			"type" : "JSON",
			"value" : {
				"type" : "object",
				"properties" : {
					"userId" : {
						"type" : "integer"
					},
					"id" : {
						"type" : "integer"
					},
					"title" : {
						"type" : "string"
					},
					"body" : {
						"type" : "string"
					}
				},
				"required" : ["userId", "id", "title", "body"]
			}
		}
	},
	"transformer" : "(input) => { return input; }"
}