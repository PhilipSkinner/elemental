{
	"name" : "pets",
	"controllers" : {
		"getPets" : "./controllers/getPets.js",
		"createPet" : "./controllers/createPet.js"
	},
	"services" : {
		"petsRepository" : {
			"type" : "singleton",
			"source" : "./services/petsRepository.js"
		},
		"dbConnection" : {
			"type" : "singleton",
			"source" : "./services/dbConnection.js"
		},
		"randomSingletonGenerator" : {
			"type" : "singleton",
			"source" : "./services/randomSingleton.js"
		},
		"randomGenerator" : {
			"type" : "transative",
			"source" : "./services/random.js"
		}
	},
	"routing" : {
		"/" : {
			"get" : {
				"controller" : "getPets"				
			},
			"post" : {
				"controller" : "createPet"				
			}
		}
	}	
}